{
  "name": "react",
  "version": "1.0.0",
  "description": "React example starter project",
  "keywords": [
    "react",
    "starter"
  ],
  "main": "src/index.js",
  "peerDependencies": {
    "eslint": "*"
  },
  "dependencies": {
    "@material-ui/core": "4.9.4",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "formik": "^2.1.4",
    "formik-material-ui": "^2.0.0-beta.1",
    "firebase": "^7.9.3",
    "jss-rtl": "^0.3.0",
    "material-ui-icons": "^1.0.0-beta.36",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-xo": "^0.31.0",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "typescript": "3.3.3",
    "yaml-lint": "^1.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "yamllint '**/*.{yaml,yml}' && eslint . --fix '**/*.{js,ts,jsx,tsx,css,json}'",
    "format": "prettier --write '**/*.{js,ts,jsx,tsx,css,json,yaml,yml}'"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --no-ignore --max-warnings 0",
      "git add"
    ],
    "*.{yaml,yml}": [
      "prettier --write",
      "yamllint",
      "git add"
    ]
  }
}
